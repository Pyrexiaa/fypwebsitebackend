generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Mother {
  id                           Int      @id @default(autoincrement())
  age                          Float
  height                       Float?
  weight                       Float?
  PreviouslyFailedPregnancy    Boolean?
  HighRiskPreeclampsia         Boolean?
  PregnancyInducedHypertension Boolean?
  PregestationalLDM            Boolean?
  GestationalLDM               Boolean?
  Smoking                      Boolean?
  Scans                        Scans[]  @relation("MotherToScans")
}

model Scans {
  id            Int     @id @default(autoincrement())
  motherId      Int
  mother        Mother  @relation("MotherToScans", fields: [motherId], references: [id])
  gender        Int
  ga            Int
  bpd           Float?
  hc            Float
  ac            Float
  fl            Float
  afi           Float?
  cpr           Float?
  psv           Float?
  efw           Float
  ute_ari       Float?
  ute_api       Float?
  umb_api       Float?
  placenta_site Int?
  af            Int?
}
